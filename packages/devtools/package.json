{
  "name": "@yorkie-js/devtools",
  "displayName": "Yorkie Devtools",
  "version": "0.6.37",
  "description": "A browser extension that helps you debug Yorkie.",
  "homepage": "https://yorkie.dev/",
  "scripts": {
    "dev": "plasmo dev --build-path=dist",
    "build": "plasmo build --build-path=dist --zip",
    "test": "plasmo test",
    "lint": "eslint . --fix --max-warnings=0 --ext .ts,.tsx --flag v10_config_lookup_from_file"
  },
  "dependencies": {
    "@microlink/react-json-view": "^1.23.0",
    "@yorkie-js/sdk": "workspace:*",
    "classnames": "^2.3.2",
    "plasmo": "0.90.5",
    "prism-react-renderer": "^2.3.1",
    "rc-slider": "^10.5.0",
    "react": "18.2.0",
    "react-arborist": "^3.4.0",
    "react-dom": "18.2.0",
    "react-resizable-layout": "^0.7.2",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "prettier": "3.0.3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "activeTab"
    ]
  }
}
