{
  "name": "@yorkie-js/mcp",
  "version": "0.6.44",
  "description": "MCP (Model Context Protocol) server for Yorkie collaborative editing",
  "type": "module",
  "bin": {
    "yorkie-mcp": "./dist/index.js"
  },
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "typings": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node scripts/bundle.mjs",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "eslint . --fix --max-warnings=0 --ext .ts,.tsx --flag v10_config_lookup_from_file",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "yorkie",
    "mcp",
    "model-context-protocol",
    "collaborative",
    "crdt",
    "realtime",
    "claude",
    "ai-assistant",
    "llm",
    "anthropic"
  ],
  "author": {
    "name": "hackerwins",
    "email": "susukang98@gmail.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yorkie-team/yorkie-js-sdk.git",
    "directory": "packages/mcp"
  },
  "bugs": {
    "url": "https://github.com/yorkie-team/yorkie-js-sdk/issues"
  },
  "homepage": "https://github.com/yorkie-team/yorkie-js-sdk#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@yorkie-js/sdk": "workspace:*",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
