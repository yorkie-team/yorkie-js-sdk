{
  "name": "@yorkie-js/prosemirror",
  "version": "0.6.48",
  "description": "ProseMirror binding for Yorkie collaborative editing",
  "main": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/yorkie-js-prosemirror.js",
    "typings": "./dist/yorkie-js-prosemirror.d.ts"
  },
  "files": [
    "dist",
    "README.md"
