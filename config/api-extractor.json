{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": "..",
  "mainEntryPointFilePath": "<projectFolder>/lib/src/yorkie.d.ts",
  "bundledPackages": [],
  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>/lib/"
  },
  "compiler": {
    "skipLibCheck": true,
    "tsconfigFilePath": "<projectFolder>/tsconfig.json"
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/dist/yorkie-js-sdk.d.untrimmed.d.ts",
    "publicTrimmedFilePath": "<projectFolder>/dist/yorkie-js-sdk.d.ts"
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
