{
  "name": "yorkie-devtools",
  "displayName": "Yorkie Devtools",
  "version": "0.0.1",
  "description": "A browser extension that helps you debug Yorkie.",
  "homepage": "https://yorkie.dev/",
  "scripts": {
    "dev": "plasmo dev --build-path=dist",
    "build": "plasmo build --build-path=dist",
    "test": "plasmo test"
  },
  "dependencies": {
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ]
  }
}
