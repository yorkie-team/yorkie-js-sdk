{
  "name": "yorkie-js-sdk",
  "version": "0.0.8",
  "description": "Yorkie JS SDK",
  "main": "./src/yorkie.ts",
  "scripts": {
    "build:proto": "protoc --js_out=import_style=commonjs:. --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:. ./src/api/yorkie.proto",
    "build": "webpack --config webpack.dev.config.js",
    "start": "webpack-dev-server --watch --config webpack.dev.config.js",
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "lint": "eslint . --fix --ext .ts",
    "prettier": "prettier --check \"./src/**/*.ts\"",
    "prettier:fix": "prettier --write \"./src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackerwins/yorkie-js-sdk.git"
  },
  "author": {
    "name": "hackerwins",
    "email": "<susukang98@gmail.com>"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hackerwins/yorkie-js-sdk/issues"
  },
  "homepage": "https://github.com/hackerwins/yorkie-js-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/long": "^4.0.1",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-source-map-support": "^1.4.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.1",
    "prettier": "2.0.5",
    "sinon": "^9.0.1",
    "ts-loader": "^6.2.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^3.1.3",
    "google-protobuf": "^3.11.4",
    "grpc-web": "^1.0.7",
    "long": "^4.0.0"
  }
}
